	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fafafa', endColorstr='#f6f4f9',GradientType=0 ); /* IE6-9 */
	color:#4b2582;
	border:1px solid #bcbcbc;
}
#tabs .ui-state-default a, #tabs .ui-widget-content .ui-state-default a, #tabs .ui-widget-header .ui-state-default a {
